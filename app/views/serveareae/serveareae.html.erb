<% @imgurl="/images/yc5.jpg" %>
<div id = "leftmenu">
  <table width="200" cellpadding="0" cellspacing="0" border="0">
  <tr>
    <td id="menutop1" height="8" width="200"><img src="/images/memu_r1_c1.jpg" width="200" height="8"></td>
  </tr>
  <tr style="cursor:pointer">
    <td id="menutop2" height="20" width="200" align="left" background="/images/memu_r2_c1.jpg" style="color:#FFFFFF;font-weight:bold;" onMouseOver="change('menutop2','/images/memu_r6_c1.jpg');" onmouseout="recover('menutop2');">&nbsp;&nbsp;&nbsp;Support</td>
  </tr>
    <% 
    for servearea in @servearea
    %>
  <tr align="left" style="cursor:pointer">
    <td name="paremenu" id="<%=servearea.categoryid%>" width="200" height="20" background="/images/memu_r9_c1.jpg" onMouseOver="change('<%=servearea.categoryid%>','/images/memu_r6_c1.jpg');" onmouseout="recover('<%=servearea.categoryid%>');" onclick="show1('child<%=servearea.categoryid%>','<%=servearea.categoryid%>','<%=servearea.url%>')">
    &nbsp;&nbsp;&nbsp;<%=servearea.categoryname%>
    </td>
  </tr>
  <tr name="childmenu" id="child<%=servearea.categoryid%>" style="display:none">
    <td>
      <table width="200" cellpadding="0" cellspacing="0" border="0">
        <% for serveareadetail in EnCategory.find(:all,:conditions => "substring(categoryid,1,6) = '" + servearea.categoryid + "' and length(categoryid) = 9",:order => "categoryid") %>
          <tr align="left" style="cursor:pointer">
          <td ame="childmenuitem" id="<%=serveareadetail.categoryid%>" width="200" height="20" background="/images/memu_r3_c1.jpg" onMouseOver="change('<%=serveareadetail.categoryid%>','/images/memu_r7_c1.jpg');" onmouseout="recover('<%=serveareadetail.categoryid%>');" onclick="show2('<%=serveareadetail.categoryid%>','<%=serveareadetail.url%>')">
          &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<%=serveareadetail.categoryname%>
          </td>
         </tr>  
        <% end %>
      </table>
     </td>
   </tr>
  <% end  %>
