<% @imgurl="/images/yc3.jpg" %>
<div id = "leftmenu">
  <table width="200" cellpadding="0" cellspacing="0" border="0">
  <tr>
    <td id="menutop1" height="8" width="200"><img src="/images/memu_r1_c1.jpg" width="200" height="8"></td>
  </tr>
  <tr style="cursor:pointer">
    <td id="menutop2" height="20" width="200" align="left" background="/images/memu_r2_c1.jpg" style="color:#FFFFFF;font-weight:bold;" onMouseOver="change('menutop2','/images/memu_r6_c1.jpg');" onmouseout="recover('menutop2');">&nbsp;&nbsp;&nbsp;About Bioer</td>
  </tr>
    <% 
    for aboutbioer in @aboutbioer
    %>

  <tr align="left" style="cursor:pointer">
    <td name="paremenu" id="<%=aboutbioer.categoryid%>" width="200" height="20" background="/images/memu_r9_c1.jpg" onMouseOver="change('<%=aboutbioer.categoryid%>','/images/memu_r6_c1.jpg');" onmouseout="recover('<%=aboutbioer.categoryid%>');" onclick="show1('child<%=aboutbioer.categoryid%>','<%=aboutbioer.categoryid%>','<%=aboutbioer.url%>')">
    &nbsp;&nbsp;&nbsp;<%=aboutbioer.categoryname%>
    </td>
  </tr>

  <tr name="childmenu" id="child<%=aboutbioer.categoryid%>" style="display:none">
    <td>
      <table width="200" cellpadding="0" cellspacing="0" border="0">
        <% for aboutbioerdetail in EnCategory.find(:all,:conditions => "substring(categoryid,1,6) = '" + aboutbioer.categoryid + "' and length(categoryid) = 9",:order => "categoryid") %>
          <tr align="left" style="cursor:pointer">
          <td name="childmenuitem" id="<%=aboutbioerdetail.categoryid%>" width="200" height="20" background="/images/memu_r3_c1.jpg" onMouseOver="change('<%=aboutbioerdetail.categoryid%>','/images/memu_r7_c1.jpg');" onmouseout="recover('<%=aboutbioerdetail.categoryid%>');" onclick="show2('<%=aboutbioerdetail.categoryid%>','<%=aboutbioerdetail.url%>')">
          &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<%=aboutbioerdetail.categoryname%>
          </td>
         </tr>  
        <% end %>
      </table>
     </td>
   </tr>
  <% end  %>
  <!--
  <tr>
    <td id="menubuttom1" height="20" width="200"><img src="/images/memu_r19_c2.jpg" width="200" height="20"></td>
  </tr>
  -->
  <tr>
  <td id ="menubuttom2" width="200"><img src="/images/memu_r11_c1.jpg" width="200" height="7"></td>
  </tr>
</table>
</div>

<div id = "productcontent">
<iframe id="frame1" onload="this.height=this.contentWindow.document.body.scrollHeight;setTop()" frameborder="0"  height="100%" width="100%" name="pcontent" src="<%=@menu%>" scrolling="no"></iframe>
</div>


