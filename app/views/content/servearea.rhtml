<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=gb2312" />
<title><%=@title%></title>
<%= stylesheet_link_tag 'content' %>
</head>

<body>
<!--技术支持-->
 <div id="yylybanner"><%=@title%></div>
 <div id="jszc">
   <div id="jszc1">
   <table cellpadding="0" cellspacing="0" align="left" width="540">
   <% if @jszc.size > 0  %>
   <% for jszc in @jszc%>
     <tr>
       <% if jszc.categoryid == "003001" %>
       <td width="30" align="left"><img src="/images/Help.png" alt="question ask" border="0" height="16" width="16" /></td>  
       <td align="left">
         <a style="cursor:pointer" onClick='window.open("questionview?id=<%=jszc.id%>","","scrollbars=yes,width=600,height=360,top=200")' onmousemove="this.style['color']='#ff0000'" onmouseout="this.style['color']='#000000'"><%=jszc.title%>
         </a>
       </td>   
      <% else %>
       <td width="30" align="left"><img src="/images/downloadimg.jpg" alt="soft download" border="0" height="20" width="26" /></td>  
       <td align="left">
         <!--<%=link_to jszc.title,:controller=>'content',:action => 'downloadfile',:id => jszc.id,:popup => ['downloadfile', 'height=300,width=600'] %>-->
         <a style="cursor:pointer" onClick='window.open("/content/downloadfile?id=<%=jszc.id%>","","scrollbars=yes,width=600,height=360,top=200")' onmousemove="this.style['color']='#ff0000'" onmouseout="this.style['color']='#000000'"><%=jszc.title%>
         </a>
       </td>   
      <% end %>
     </tr>
   <% end %>
   <% else %>
     <tr><td align="left"></td></tr>
   <% end %>
   </table>
 </div>
    <br/>
     <br/>
    <div id="jszc1">
   <table id="fy" cellpadding="0" cellspacing="0" align="right" width="550" >
     <tr><td>
    <%= will_paginate @jszc,
    :inner_window => 1,
    :outer_window => 1,
    :prev_label => '上一页',
    :next_label => '下一页'
    %>
    </td>
    </tr>
    </table>
  </div>
 </div>


</body>
</html>